include_rules

: | ../util/makedefs |> ../util/makedefs -m |> monstr.c
: | ../util/makedefs |> ../util/makedefs -z %o |> vis_tab.cc

: foreach *.cc | ../include/pm.h ../include/onames.h ../include/date.h |> !cc |>
: foreach *.c |> !cc |>

: *.o ../base/base.a ../entitylib/monst.o ../entitylib/objects.o |> g++ -o %o %f |> nethacked
